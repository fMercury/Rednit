al":{"line":57,"column":28},"generated":{"line":1,"column":2044}},{"source":"views/MainScreenView.js","name":"PropTypes","original":{"line":58,"column":10},"generated":{"line":1,"column":2049}},{"source":"views/MainScreenView.js","name":"PropTypes","original":{"line":58,"column":10},"generated":{"line":1,"column":2056}},{"source":"views/MainScreenView.js","name":"array","original":{"line":58,"column":20},"generated":{"line":1,"column":2058}},{"source":"views/MainScreenView.js","name":"array","original":{"line":58,"column":20},"generated":{"line":1,"column":2066}},{"source":"views/MainScreenView.js","name":"MainScreenView","original":{"line":61,"column":15},"generated":{"line":1,"column":2073}},{"source":"views/MainScreenView.js","name":"MainScreenView","original":{"line":61,"column":15},"generated":{"line":1,"column":2081}},{"source":"views/MainScreenView.js","name":"MainScreenView","original":{"line":61,"column":15},"generated":{"line":1,"column":2089}}],"sources":{"views/MainScreenView.js":"import React, {Component} from 'react';\nimport PropTypes from 'prop-types';\nimport Blockies from 'react-blockies';\n\nclass MainScreenView extends Component {\n  renderEvent(event) {\n    return (\n      <p className=\"click-history-item\" key={event.key}>\n        <span className=\"date\"> {event.pressTime} ago </span>\n        <span className=\"bold user-inline\">\n          <Blockies seed={event.address.toLowerCase()} size={8} scale={4} />{' '}\n          {event.name}\n        </span>{' '}\n        pressed at {event.score} seconds\n      </p>\n    );\n  }\n\n  render() {\n    return (\n      <div className=\"main-screen\">\n        <div className=\"container text-center\">\n          <p>\n            You have{' '}\n            <span className=\"bold\">\n              {this.props.clicksLeft} <em>GasTokens</em>\n            </span>{' '}\n            left\n          </p>\n          <button\n            className=\"btn main-screen-btn\"\n            onClick={this.props.onClickerClick}\n          >\n            click here\n          </button>\n\n          <p className=\"click-cost\">\n            Costs 1 <em>klik</em>\n          </p>\n          <p className=\"last-click-text\">\n            Last time someone pressed this button was\n            <span className=\"bold\"> {this.props.lastClick}</span> ago\n          </p>\n          <hr className=\"separator\" />\n          <div className=\"click-history\">\n            {this.props.events.map(this.renderEvent.bind(this))}\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nMainScreenView.propTypes = {\n  clicksLeft: PropTypes.number,\n  lastClick: PropTypes.string,\n  onClickerClick: PropTypes.func,\n  events: PropTypes.array\n};\n\nexport default MainScreenView;\n"},"lineCount":null}},"hash":"9e08f0599f67f9085e59868e8c77e132","cacheData":{"env":{}}}